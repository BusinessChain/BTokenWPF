<UserControl x:Class="BTokenWPF.ListBoxItemHeader"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BTokenWPF"
             mc:Ignorable="d">
    <Grid Name="Grid">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="60"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Label Name="LabelHeight" Grid.Row="0" Grid.Column="0"/>
        <Label Name="LabelHash" Grid.Row="0" Grid.Column="1"/>
        <Label Name="LabelHashChild" Grid.Row="1" Grid.Column="1" Margin="5,-10,0,0"/>
    </Grid>
</UserControl>
